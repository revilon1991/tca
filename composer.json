{
    "name": "revilon1991/tca",
    "type": "project",
    "description": "Telegram Channel/Chat Analytics",
    "keywords": ["telegram", "channel", "chat", "analytics"],
    "license": "proprietary",
    "authors": [
        {"name": "Evgeniy Kuznetsov", "email": "revil-on@mail.ru", "homepage": "https://9ek.ru/"}
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gmp": "*",
        "ext-iconv": "*",
        "amphp/dns": "dev-master#aa1892bd as 0.9",
        "danog/madelineproto": "dev-master",
        "gedmo/doctrine-extensions": "^2.4",
        "mybuilder/cronos-bundle": "^2.1",
        "sensio/framework-extra-bundle": "^5.4",
        "sonata-project/admin-bundle": "^3.52",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "4.3.*",
        "wakeapp/dto-resolver": "^1.1"
    },
    "require-dev": {
        "symfony/var-dumper": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/danog/phpseclib"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
